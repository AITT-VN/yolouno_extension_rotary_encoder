<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category id="rotary" name="ROTARY" colour="#A39400" icon="spinner">
    <block type="yolouno_rotary_encoder_init"></block>
    <block type="yolouno_rotary_encoder_get_value"></block>
    <block type="yolouno_rotary_encoder_set_range_value">
      <value name="min">
          <shadow type="math_number"><field name="NUM">0</field></shadow>
      </value>
      <value name="max">
          <shadow type="math_number"><field name="NUM">100</field></shadow>
      </value>
    </block>
    <block type="yolouno_rotary_encoder_mode"></block>
    <block type="yolouno_rotary_encoder_set_current_value">
      <value name="value">
          <shadow type="math_number"><field name="NUM">0</field></shadow>
      </value>
    </block>
    <block type="yolouno_rotary_encoder_direction" />
  </category>
</xml>